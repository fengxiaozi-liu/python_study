"<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Document</title>\n    <!-- \u5bfc\u5165jquery\u5bf9\u5e94\u7684js\u6587\u4ef6 -->\n    <script src=\"./jquery-1.12.4.min.js\"></script>\n    <!-- \u4f7f\u7528jquery\u4ee3\u7801\u9700\u8981\u91cd\u65b0\u5f00\u4e00\u4e2ascript\u6807\u7b7e -->\n    <script>\n        // $\u662fjquery\u7684\u8c61\u5f81\uff0c$\u672c\u8d28\u4e0a\u5c31\u662f\u4e00\u4e2a\u51fd\u6570\n        $(document).ready(function(){\n            // \u83b7\u53d6\u6807\u7b7e\u548ccss\u6837\u5f0f\u5339\u914d\u6807\u7b7e\u7684\u89c4\u5219\u4e00\u6837\n            //  \u4ee5\u540e\u4f7f\u7528jquery\uff0c\u53d8\u91cf\u540d\u90fd\u8981\u4ee5$\u7b26\u53f7\u5f00\u5934  \n           var $div = $(\"#div1\");\n           alert($div);\n        });\n        // ready \u7b49\u5f85\u9875\u9762\u6807\u7b7e\u52a0\u8f7d\u5b8c\u6210\u4ee5\u540e\u5c31\u6267\u884cready\u4e8b\u4ef6\uff0c\u4e0d\u4f1a\u7b49\u5f85\u8d44\u6e90\u6570\u636e\u52a0\u8f7d\u5b8c\u6210\n\n        // jquery\u7684\u7b80\u5199\u65b9\u5f0f\n        $(function(){\n            var $div2=$(\"div2\");\n            alert($div2);\n        })   \n    </script>\n</head>\n<body>\n    <div id=\"div1\">\u54c8\u54c8</div>\n    <div id=\"div2\">\u6211\u662f\u7b80\u5199\u7684div\u6807\u7b7e</div>\n</body>\n</html>"